{
  "name": "@kosmic/koa",
  "version": "0.0.0",
  "description": "Typescript first drop in replacement for koa",
  "keywords": [
    "koa",
    "typescript",
    "koajs",
    "kosmic",
    "@kosmic",
    "@kosmic/koa",
    "framework",
    "server",
    "http",
    "node",
    "node.js"
  ],
  "license": "MIT",
  "author": "Spencer Snyder <sasnyde2@gmail.com> (https://spencersnyder.io/)",
  "main": "dist/application.js",
  "types": "dist/application.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.json",
    "dev": "npm build --watch",
    "prepublishOnly": "npm run build",
    "test": "npm run build && jest --forceExit"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*.ts",
      "!test/fixtures/**/*"
    ],
    "require": [
      "ts-node/register"
    ],
    "verbose": true
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/.history/"
    ]
  },
  "dependencies": {
    "accepts": "^1.3.5",
    "cache-content-type": "^1.0.0",
    "content-disposition": "~0.5.2",
    "content-type": "^1.0.4",
    "cookies": "~0.8.0",
    "debug": "^4.3.2",
    "delegates": "^1.0.0",
    "destroy": "^1.0.4",
    "encodeurl": "^1.0.2",
    "escape-html": "^1.0.3",
    "fresh": "~0.5.2",
    "http-assert": "^1.3.0",
    "http-errors": "^1.6.3",
    "on-finished": "^2.3.0",
    "parseurl": "^1.3.2",
    "statuses": "^1.5.0",
    "type-is": "^1.6.16",
    "vary": "^1.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/accepts": "^1.3.5",
    "@types/content-disposition": "^0.5.5",
    "@types/content-type": "^1.1.5",
    "@types/cookies": "^0.7.7",
    "@types/debug": "^4.1.8",
    "@types/delegates": "^1.0.0",
    "@types/destroy": "^1.0.0",
    "@types/encodeurl": "^1.0.0",
    "@types/escape-html": "^1.0.2",
    "@types/fresh": "^0.5.0",
    "@types/http-assert": "^1.5.3",
    "@types/http-errors": "^2.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "~20.3.2",
    "@types/on-finished": "^2.3.1",
    "@types/parseurl": "^1.3.1",
    "@types/statuses": "^1.5.0",
    "@types/type-is": "^1.6.3",
    "@types/vary": "^1.1.0",
    "ava": "^5.3.1",
    "husky": "^8.0.3",
    "jest": "^28.1.2",
    "lint-staged": "^13.2.2",
    "npm-package-json-lint": "^6.4.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "prettier-plugin-packagejson": "^2.4.3",
    "rimraf": "^5.0.1",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "type-fest": "^4.2.0",
    "typescript": "~5.1.3",
    "xo": "^0.54.2"
  }
}
